\textsc{Answer:}
\begin{answercode}
#include <iostream>

int sum(int n)
{
    int s = 0;
    int * i = new int;
    for (*i = 0; *i <= n; ++(*i))
    {
        s += *i;
    }
    return s;
}

int main()
{
    std::cout << sum(10) << '\n';
    return 0;
}
